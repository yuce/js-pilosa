{
  "name": "pilosa",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail test/*.spec.ts",
    "integration-test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail --timeout 4000 integration-test/*.it.ts",
    "test-all": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail --timeout 4000 test/*.spec.ts integration-test/*.it.ts",
    "cover": "NODE_ENV=test nyc npm run test-all",
    "generate-proto": "pbjs -t static-module -w commonjs -o internal/internal.js internal/internal.proto && pbts -o internal/internal.d.ts internal/internal.js"
  },
  "keywords": [],
  "author": "YÃ¼ce Tekol <yucetekol@gmail.com> (http://yuce.me)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^2.2.39",
    "@types/nock": "^8.2.1",
    "@types/node": "^7.0.8",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.9",
    "nyc": "^10.1.2",
    "protobufjs": "^6.6.5",
    "ts-node": "^2.1.0",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "typings",
      "internal",
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "json",
      "html"
    ],
    "all": true
  }
}
